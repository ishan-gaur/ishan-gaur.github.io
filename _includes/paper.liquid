<div class="paper">
  <h3>{{ paper.data.title }}</h3>
  <p><em>
    {{ paper.data.venue }}, {{ paper.data.pub_date }}
    {% if paper.data.awards %}
      ({{ paper.data.awards }})
    {% endif %}
  </em></p>
  <p>{{ paper.data.authors }}</p>
  {% if paper.data.equal_contribution_note %}
    <p class="note">{{ paper.data.equal_contribution_note }}</p>
  {% endif %}
  {% if paper.data.links %}
    <div class="links">
        {%- for link in paper.data.links -%}
            <a href="{{ link.url }}" target="_blank">{{ link.text }}</a>
            {%- unless forloop.last %} | {% endunless -%}
        {%- endfor -%}
    </div>
  {% endif %}
</div>
